{"code":"import styled from 'styled-components';\r\nimport React from 'react';\r\nimport { AvatarDao } from '../avatar';\r\nexport const ListItemAction = ({ title, subtitle, iconLeft, iconRight, mode = 'default', ...props }) => {\r\n    return (React.createElement(Container, Object.assign({}, props, { mode: mode, \"data-testid\": \"listItem-action\" }),\r\n        React.createElement(LeftContent, null,\r\n            React.createElement(RenderIconLeft, { icon: iconLeft, label: title }),\r\n            React.createElement(LabelContainer, null,\r\n                React.createElement(\"p\", { className: \"font-bold truncate ft-text-base\" }, title),\r\n                subtitle && React.createElement(\"p\", { className: \"truncate ft-text-sm\" }, subtitle))),\r\n        iconRight && React.createElement(\"span\", null, iconRight)));\r\n};\r\n// NOTE: Temporary, to be refactored with new version of ODS\r\nconst RenderIconLeft = ({ icon, label }) => {\r\n    if (!icon) {\r\n        return null;\r\n    }\r\n    return typeof icon === 'string' ? (React.createElement(AvatarDao, { daoName: label || icon, src: icon, size: \"small\" })) : (React.createElement(\"span\", null, icon));\r\n};\r\nconst Container = styled.button.attrs(({ mode, bgWhite = false }) => {\r\n    const baseLayoutClasses = 'flex items-center gap-x-1.5 w-full';\r\n    const baseStyleClasses = 'py-1.5 px-2 rounded-xl font-normal';\r\n    let className = `${baseLayoutClasses} ${baseStyleClasses}`;\r\n    switch (mode) {\r\n        case 'disabled':\r\n            className += ' text-ui-300 border-ui-200';\r\n            className += bgWhite ? ' bg-ui-0' : ' bg-ui-50';\r\n            break;\r\n        case 'selected':\r\n            className += ' text-primary-500 border-primary-500';\r\n            className += bgWhite ? ' bg-primary-50' : ' bg-ui-0';\r\n            break;\r\n        default:\r\n            {\r\n                const focusClasses = 'focus:outline-none focus-visible:ring-2 focus-visible:ring-primary-500';\r\n                const hoverClasses = 'hover:text-primary-500';\r\n                let activeClasses = 'active:outline-none active:ring-0';\r\n                activeClasses += bgWhite\r\n                    ? ' active:bg-primary-50'\r\n                    : ' active:bg-ui-0';\r\n                className += bgWhite ? ' bg-ui-0' : ' bg-ui-50';\r\n                className += ` text-ui-600 ${activeClasses} ${focusClasses} ${hoverClasses}`;\r\n            }\r\n            break;\r\n    }\r\n    const disabled = mode === 'disabled';\r\n    return { className, disabled };\r\n}) ``;\r\nconst LabelContainer = styled.div.attrs({\r\n    className: 'text-left overflow-hidden',\r\n}) ``;\r\nconst LeftContent = styled.div.attrs({\r\n    className: 'flex items-center space-x-1.5 flex-1 overflow-hidden',\r\n}) ``;\r\n//# sourceMappingURL=action.js.map","references":["/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/styled-components/index.d.ts","/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/react/index.d.ts","/root/repo/chainlink-spring-hackathon/frontend/packages/ui-components/src/components/avatar/index.ts"],"map":"{\"version\":3,\"file\":\"action.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/components/listItem/action.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,MAAM,MAAM,mBAAmB,CAAC;AACvC,OAAO,KAA6B,MAAM,OAAO,CAAC;AAElD,OAAO,EAAC,SAAS,EAAC,MAAM,WAAW,CAAC;AAgCpC,MAAM,CAAC,MAAM,cAAc,GAAkC,CAAC,EAC5D,KAAK,EACL,QAAQ,EACR,QAAQ,EACR,SAAS,EACT,IAAI,GAAG,SAAS,EAChB,GAAG,KAAK,EACT,EAAE,EAAE;IACH,OAAO,CACL,oBAAC,SAAS,oBAAK,KAAK,IAAE,IAAI,EAAE,IAAI,iBAAc,iBAAiB;QAC7D,oBAAC,WAAW;YACV,oBAAC,cAAc,IAAC,IAAI,EAAE,QAAQ,EAAE,KAAK,EAAE,KAAK,GAAI;YAGhD,oBAAC,cAAc;gBACb,2BAAG,SAAS,EAAC,iCAAiC,IAAE,KAAK,CAAK;gBACzD,QAAQ,IAAI,2BAAG,SAAS,EAAC,qBAAqB,IAAE,QAAQ,CAAK,CAC/C,CACL;QACb,SAAS,IAAI,kCAAO,SAAS,CAAQ,CAC5B,CACb,CAAC;AACJ,CAAC,CAAC;AAEF,4DAA4D;AAC5D,MAAM,cAAc,GAGf,CAAC,EAAC,IAAI,EAAE,KAAK,EAAC,EAAE,EAAE;IACrB,IAAI,CAAC,IAAI,EAAE;