{"code":"import React from 'react';\r\nimport styled from 'styled-components';\r\nimport { shortenAddress } from '../../utils/addresses';\r\nimport { AlertInline } from '../alerts';\r\nimport { AvatarDao } from '../avatar';\r\nimport { IconClock } from '../icons';\r\nimport { Link } from '../link';\r\nimport { LinearProgress } from '../progress';\r\nimport { Tag } from '../tag';\r\nexport function isExploreProposal(proposalUseCase) {\r\n    return proposalUseCase === 'explore';\r\n}\r\nexport const CardProposal = ({ process = 'pending', title, description, voteTitle, voteProgress, voteLabel, tokenAmount, tokenSymbol, winningOptionValue, publishLabel, publisherAddress, explorer = 'https://etherscan.io/', alertMessage, stateLabel, type = 'list', daoLogo, daoName, onClick, }) => {\r\n    const addressExploreUrl = `${explorer}address/${publisherAddress}`;\r\n    return (React.createElement(Card, { \"data-testid\": \"cardProposal\", onClick: onClick },\r\n        React.createElement(Header, null,\r\n            React.createElement(HeaderOptions, { process: process, stateLabel: stateLabel, alertMessage: alertMessage, type: type })),\r\n        React.createElement(TextContent, null,\r\n            React.createElement(Title, null, title),\r\n            React.createElement(Description, null, description),\r\n            React.createElement(Publisher, null,\r\n                isExploreProposal(type) ? (React.createElement(AvatarDao, { daoName: daoName, size: \"small\", src: daoLogo })) : (React.createElement(PublisherLabel, null, publishLabel)),\r\n                React.createElement(Link, { external: true, href: addressExploreUrl, label: shortenAddress((isExploreProposal(type) ? daoName : publisherAddress) || ''), className: \"text-sm\" }))),\r\n        process === 'active' && voteProgress !== undefined && (React.createElement(LoadingContent, null,\r\n            React.createElement(ProgressInfoWrapper, null,\r\n                React.createElement(ProgressTitle, null, voteTitle),\r\n                React.createElement(Amount, null, tokenAmount && tokenSymbol\r\n                    ? `${tokenAmount} ${tokenSymbol}`\r\n                    : winningOptionValue)),\r\n            React.createElement(LinearProgress, { max: 100, value: voteProgress }),\r\n            React.createElement(ProgressInfoWrapper, null,\r\n                React.createElement(Vote, null, voteLabel),\r\n                React.createElement(Percentage, null,\r\n                    voteProgress,\r\n                    \"%\"))))));\r\n};\r\nconst HeaderOptions = ({ alertMessage, process, stateLabel, type, }) => {\r\n    switch (process) {\r\n        case 'draft':\r\n            return React.createElement(Tag, { label: stateLabel[0] });\r\n        case 'pending':\r\n            return (React.createElement(React.Fragment, null,\r\n                React.createElement(Tag, { label: stateLabel[1] }),\r\n                alertMessage && (React.createElement(AlertInline, { label: alertMessage, icon: React.createElement(IconClock, { className: \"text-info-500\" }), mode: \"neutral\" }))));\r\n        case 'active':\r\n            return (React.createElement(React.Fragment, null,\r\n                !isExploreProposal(type) && (React.createElement(Tag, { label: stateLabel[2], colorScheme: 'info' })),\r\n                alertMessage && (React.createElement(AlertInline, { label: alertMessage, icon: React.createElement(IconClock, { className: \"text-info-500\" }), mode: \"neutral\" }))));\r\n        case 'executed':\r\n            return React.createElement(Tag, { label: stateLabel[3], colorScheme: 'success' });\r\n        case 'succeeded':\r\n            return React.createElement(Tag, { label: stateLabel[4], colorScheme: 'success' });\r\n        case 'defeated':\r\n            return React.createElement(Tag, { label: stateLabel[5], colorScheme: 'critical' });\r\n        default:\r\n            return null;\r\n    }\r\n};\r\nconst Card = styled.button.attrs({\r\n    className: 'w-full bg-white rounded-xl p-2 space-y-3 box-border ' +\r\n        'hover:border hover:border-ui-100 ' +\r\n        'active:border active:border-ui-200 ' +\r\n        'focus:outline-none focus:ring-2 focus:ring-primary-500',\r\n}) `\r\n  &:hover {\r\n    box-shadow: 0px 4px 8px rgba(31, 41, 51, 0.04),\r\n      0px 0px 2px rgba(31, 41, 51, 0.06), 0px 0px 1px rgba(31, 41, 51, 0.04);\r\n  }\r\n`;\r\nconst Header = styled.div.attrs({\r\n    className: 'flex justify-between',\r\n}) ``;\r\nconst Title = styled.p.attrs({\r\n    className: 'text-ui-800 text-left font-bold ft-text-xl',\r\n}) ``;\r\nconst Description = styled.p.attrs({\r\n    className: 'text-ui-600 text-left font-normal ft-text-base line-clamp-2',\r\n}) ``;\r\nconst Publisher = styled.span.attrs({\r\n    className: 'flex space-x-1 text-ui-500 ft-text-sm',\r\n}) ``;\r\nconst TextContent = styled.div.attrs({\r\n    className: 'space-y-1.5',\r\n}) ``;\r\nconst LoadingContent = styled.div.attrs({\r\n    className: 'space-y-2 p-2 bg-ui-50 rounded-xl',\r\n}) ``;\r\nconst ProgressInfoWrapper = styled.div.attrs({\r\n    className: 'flex justify-between',\r\n}) ``;\r\nconst ProgressTitle = styled.h3.attrs({\r\n    className: 'text-ui-800 ft-text-base font-bold',\r\n}) ``;\r\nconst Amount = styled.span.attrs({\r\n    className: 'text-ui-500 ft-text-base',\r\n}) ``;\r\nconst Vote = styled.span.attrs({\r\n    className: 'text-primary-500 font-bold ft-text-base',\r\n}) ``;\r\nconst Percentage = styled.span.attrs({\r\n    className: 'text-primary-500 font-bold ft-text-base',\r\n}) ``;\r\nconst PublisherLabel = styled.p.attrs({ className: '-mr-0.5' }) ``;\r\n//# sourceMappingURL=cardProposal.js.map","references":["/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/react/index.d.ts","/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/styled-components/index.d.ts","/root/repo/chainlink-spring-hackathon/frontend/packages/ui-components/src/utils/addresses.tsx","/root/repo/chainlink-spring-hackathon/f