{"code":"import React from 'react';\r\nimport styled from 'styled-components';\r\nimport { TableCell } from './tableCell';\r\nimport { Tag } from '../tag';\r\nimport { IconChevronDown } from '../icons';\r\nimport { Link } from '../link';\r\nimport { shortenAddress } from '../../utils/addresses';\r\nconst colorScheme = (option) => option === 'yes' || option === 'approved'\r\n    ? 'success'\r\n    : option === 'no'\r\n        ? 'critical'\r\n        : 'neutral';\r\nexport const VotersTable = ({ voters, onLoadMore, page = 1, showOption = false, showVotingPower = false, showAmount = false, pageSize = 3, }) => {\r\n    const displayedVoters = page * pageSize;\r\n    return (React.createElement(\"div\", { className: \"overflow-x-auto\" },\r\n        React.createElement(Table, { \"data-testid\": \"votersTable\" },\r\n            React.createElement(\"thead\", null,\r\n                React.createElement(\"tr\", null,\r\n                    React.createElement(TableCell, { type: \"head\", text: \"Wallet\", className: \"w-1/2\" }),\r\n                    showOption && React.createElement(TableCell, { type: \"head\", text: \"Option\" }),\r\n                    React.createElement(TableCell, { type: \"head\", rightAligned: true }),\r\n                    showVotingPower && (React.createElement(TableCell, { type: \"head\", text: \"Voting Power\", className: \"text-center\", rightAligned: true })),\r\n                    showAmount && (React.createElement(TableCell, { type: \"head\", text: \"Token Amount\", rightAligned: true })))),\r\n            React.createElement(\"tbody\", null,\r\n                voters.slice(0, displayedVoters).map((voter, index) => (React.createElement(\"tr\", { key: index },\r\n                    React.createElement(TableCell, { type: \"text\", text: shortenAddress(voter.wallet) }),\r\n                    showOption && (React.createElement(TableCell, { type: \"tag\" }, voter.option && (React.createElement(\"span\", { className: \"flex\" },\r\n                        React.createElement(Tag, { label: voter.option, className: \"capitalize\", colorScheme: colorScheme(voter.option) }))))),\r\n                    showOption && voter.voteReplaced === true ? (React.createElement(TableCell, { type: \"tag\", rightAligned: true },\r\n                        React.createElement(\"span\", { className: \"flex items-center -ml-2 text-xs text-ui-600\" }, \"Edited\"))) : (React.createElement(TableCell, { type: \"text\", rightAligned: true })),\r\n                    showVotingPower && (React.createElement(TableCell, { type: \"text\", text: voter.votingPower, rightAligned: true })),\r\n                    showAmount && (React.createElement(TableCell, { type: \"text\", text: voter.tokenAmount, rightAligned: true }))))),\r\n                onLoadMore &&\r\n                    voters.length > pageSize &&\r\n                    displayedVoters < voters.length && (React.createElement(\"tr\", null,\r\n                    React.createElement(TableCell, { type: \"link\" },\r\n                        React.createElement(Link, { label: \"Load More\", iconRight: React.createElement(IconChevronDown, null), onClick: onLoadMore })),\r\n                    showOption && React.createElement(TableCell, { type: \"text\", text: \"\" }),\r\n                    showVotingPower && React.createElement(TableCell, { type: \"text\", text: \"\" }),\r\n                    showAmount && React.createElement(TableCell, { type: \"text\", text: \"\" })))))));\r\n};\r\nexport const Table = styled.table.attrs({\r\n    className: 'w-full border-separate whitespace-nowrap',\r\n}) `\n  border-spacing: 0;\n\n  tr th,\n  tr td {\n    border-bottom: 1px solid #e4e7eb;\n  }\n\n  tr td:first-child {\n    border-left: 1px solid #e4e7eb;\n  }\n\n  tr td:last-child {\n    border-right: 1px solid #e4e7eb;\n  }\n\n  tr th {\n    border-top: 1px solid #e4e7eb;\n  }\n\n  tr:first-child th:first-child {\n    border-top-left-radius: 12px;\n  }\n\n  tr:first-child th:last-child {\n    border-top-right-radius: 12px;\n  }\n\n  tr:last-child td:first-child {\n    border-bottom-left-radius: 12px;\n  }\n\n  tr:last-child td:last-child {\n    border-bottom-right-radius: 12px;\n  }\n`;\r\n//# sourceMappingURL=votersTable.js.map","references":["/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/react/index.d.ts","/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/styled-components/index.d.ts","/root/repo/chainlink-spring-hackathon/frontend/packages/ui-components/src/components/table/tableCell.tsx","/root/repo/chainlink-spring-hackathon/frontend/packages/ui-components/src/components/tag/index.ts","/root/repo/chainlink-spring-hackathon/frontend/packages/ui-components/src/components/icons/index.tsx","/root/repo/chainlink-spring-hackathon/frontend/packages/ui-components/src/components/link/index.ts","/root/repo/chainlink-spring-hackathon/frontend/packages/ui-components/src/utils/addresses.tsx"],"map":"{\"version\":3,\"file\":\"votersTable.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/components/table/votersTable.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,MAAM,OAAO,CAAC;AAC1B,OAAO,MAAM,MAAM,mBAAmB,CAAC;AAEvC,OAAO,EAAC,SAAS,EAAC,MAAM,aAAa,CAAC;AACtC,OAAO,EAAC,GAAG,EAAC,MAAM,QAAQ,CAAC;AAC3B,OAAO,EAAC,eAAe,EAAC,MAAM,UAAU,CAAC;AACzC,OAAO,EAAC,IAAI,EAAC,MAAM,SAAS,CAAC;AAC7B,OAAO,EAAC,cAAc,EAAC,MAAM,uBAAuB,CAAC;AAoBrD,MAAM,WAAW,GAAG,CAAC,MAAc,EAAE,EAAE,CACrC,MAAM,KAAK,KAAK,IAAI,MAAM,KAAK,UAAU;IACvC,CAAC,CAAC,SAAS;IACX,CAAC,CAAC,MAAM,KAAK,IAAI;QACjB,CAAC,CAAC,UAAU;QACZ,CAAC,CAAC,SAAS,CAAC;AAEhB,MAAM,CAAC,MAAM,WAAW,GAA+B,CAAC,EACtD,MAAM,EACN,UAAU,EACV,IAAI,GAAG,CAAC,EACR,UAAU,GAAG,KAAK,EAClB,eAAe,GAAG,KAAK,EACvB,UAAU,GAAG,KAAK,EAClB,QAAQ,GAAG,CAAC,GACb,EAAE,EAAE;IACH,MAAM,eAAe,GAAG,IAAI,GAAG,QAAQ,CAAC;IAExC,OAAO,CACL,6BAAK,SAAS,EAAC,iBAAiB;QAC9B,oBAAC,KAAK,mBAAa,aAAa;YAC9B;gBACE;oBACE,oBAAC,SAAS,IAAC,IAAI,EAAC,MAAM,EAAC,IAAI,EAAC,QAAQ,EAAC,SAAS,EAAC,OAAO,GAAG;oBACxD,UAAU,IAAI,oBAAC,SAAS,IAAC,IAAI,EAAC,MAAM,E