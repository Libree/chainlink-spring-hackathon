{"code":"import React, { useState, useEffect } from 'react';\r\nimport styled from 'styled-components';\r\nimport { IconChevronRight, IconChevronLeft } from '../icons/interface';\r\nimport { ButtonText, ButtonIcon } from '../button';\r\n/**\r\n * Default UI component\r\n */\r\nexport const Pagination = ({ totalPages = 10, activePage = 1, bgWhite = false, distance = 3, onChange, }) => {\r\n    const [page, setPage] = useState(activePage);\r\n    useEffect(() => {\r\n        onChange && onChange(page);\r\n    }, [onChange, page]);\r\n    function ButtonList() {\r\n        const list = [];\r\n        if (totalPages <= 7) {\r\n            for (let i = 1; i <= totalPages; i++) {\r\n                list.push(React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", isActive: page === i, onClick: () => setPage(i) }, (bgWhite && { bgWhite }), { label: `${i}`, key: i })));\r\n            }\r\n            return React.createElement(React.Fragment, null, list);\r\n        }\r\n        if (page - 1 <= distance) {\r\n            for (let i = 1; i <= 5; i++) {\r\n                list.push(React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", isActive: page === i, onClick: () => setPage(i) }, (bgWhite && { bgWhite }), { label: `${i}`, key: i })));\r\n            }\r\n            return (React.createElement(React.Fragment, null,\r\n                list,\r\n                React.createElement(Separator, null, \"...\"),\r\n                React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", onClick: () => setPage(totalPages) }, (bgWhite && { bgWhite }), { label: `${totalPages}` }))));\r\n        }\r\n        if (totalPages - page <= distance) {\r\n            for (let i = totalPages - 4; i <= totalPages; i++) {\r\n                list.push(React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", isActive: page === i, onClick: () => setPage(i) }, (bgWhite && { bgWhite }), { label: `${i}`, key: i })));\r\n            }\r\n            return (React.createElement(React.Fragment, null,\r\n                React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", onClick: () => setPage(1) }, (bgWhite && { bgWhite }), { label: `${1}` })),\r\n                React.createElement(Separator, null, \"...\"),\r\n                list));\r\n        }\r\n        for (let i = page - 1; i <= page + 1; i++) {\r\n            list.push(React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", isActive: page === i, onClick: () => setPage(i) }, (bgWhite && { bgWhite }), { label: `${i}`, key: i })));\r\n        }\r\n        return (React.createElement(React.Fragment, null,\r\n            React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", onClick: () => setPage(1) }, (bgWhite && { bgWhite }), { label: '1' })),\r\n            React.createElement(Separator, null, \"...\"),\r\n            list,\r\n            React.createElement(Separator, null, \"...\"),\r\n            React.createElement(ButtonText, Object.assign({ mode: \"secondary\", size: \"large\", onClick: () => setPage(totalPages) }, (bgWhite && { bgWhite }), { label: `${totalPages}` }))));\r\n    }\r\n    return (React.createElement(HStack, { \"data-testid\": \"pagination\" },\r\n        React.createElement(ButtonIcon, Object.assign({ mode: \"secondary\", size: \"large\", onClick: () => setPage(page - 1), disabled: page === 1, icon: React.createElement(IconChevronLeft, null) }, (bgWhite && { bgWhite }))),\r\n        React.createElement(ButtonList, null),\r\n        React.createElement(ButtonIcon, Object.assign({ mode: \"secondary\", size: \"large\", onClick: () => setPage(page + 1), disabled: page === totalPages, icon: React.createElement(IconChevronRight, null) }, (bgWhite && { bgWhite })))));\r\n};\r\nconst HStack = styled.div.attrs({\r\n    className: 'flex space-x-1.5',\r\n}) ``;\r\nconst Separator = 