{"code":"import React, { useEffect, useMemo, useState } from 'react';\r\nimport styled from 'styled-components';\r\nexport const AvatarDao = ({ daoName, src, size = 'medium', onClick, ...props }) => {\r\n    const [error, setError] = useState(false);\r\n    useEffect(() => {\r\n        setError(false);\r\n    }, [src]);\r\n    const daoInitials = useMemo(() => {\r\n        // To allow for no name daos - should not be a thing\r\n        if (!daoName)\r\n            return '';\r\n        const arr = daoName.trim().split(' ');\r\n        if (arr.length === 1)\r\n            return arr[0][0];\r\n        else\r\n            return arr[0][0] + arr[1][0];\r\n    }, [daoName]);\r\n    return error || !src ? (React.createElement(FallBackAvatar, Object.assign({ onClick: onClick, size: size }, props),\r\n        React.createElement(DaoInitials, null, daoInitials?.toUpperCase()))) : (React.createElement(Avatar, Object.assign({ src: src, size: size, alt: \"dao avatar\", onClick: onClick, onError: () => setError(true) }, props)));\r\n};\r\nconst sizes = {\r\n    small: 'w-3 h-3 ft-text-xs',\r\n    medium: 'w-6 h-6 ft-text-base',\r\n    big: 'w-10 h-10 ft-text-lg',\r\n    hero: 'w-14 h-14 ft-text-xl',\r\n};\r\nconst Avatar = styled.img.attrs(({ size }) => ({\r\n    className: `${size !== 'unset' && sizes[size]} rounded-full`,\r\n})) ``;\r\nconst FallBackAvatar = styled.div.attrs(({ size }) => ({\r\n    className: 'flex items-center justify-center font-bold text-ui-0 bg-gradient-to-r' +\r\n        ` from-primary-500 to-primary-800 ${size !== 'unset' && sizes[size]} rounded-full border`,\r\n})) ``;\r\nconst DaoInitials = styled.p.attrs({\r\n    className: 'w-4 h-4 flex items-center justify-center',\r\n}) ``;\r\n//# sourceMappingURL=avatarDao.js.map","references":["/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/react/index.d.ts","/root/repo/chainlink-spring-hackathon/frontend/node_modules/@types/styled-components/index.d.ts"],"map":"{\"version\":3,\"file\":\"avatarDao.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/components/avatar/avatarDao.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAiB,SAAS,EAAE,OAAO,EAAE,QAAQ,EAAC,MAAM,OAAO,CAAC;AAC1E,OAAO,MAAM,MAAM,mBAAmB,CAAC;AASvC,MAAM,CAAC,MAAM,SAAS,GAA6B,CAAC,EAClD,OAAO,EACP,GAAG,EACH,IAAI,GAAG,QAAQ,EACf,OAAO,EACP,GAAG,KAAK,EACT,EAAE,EAAE;IACH,MAAM,CAAC,KAAK,EAAE,QAAQ,CAAC,GAAG,QAAQ,CAAC,KAAK,CAAC,CAAC;IAE1C,SAAS,CAAC,GAAG,EAAE;QACb,QAAQ,CAAC,KAAK,CAAC,CAAC;IAClB,CAAC,EAAE,CAAC,GAAG,CAAC,CAAC,CAAC;IAEV,MAAM,WAAW,GAAG,OAAO,CAAC,GAAG,EAAE;QAC/B,oDAAoD;QACpD,IAAI,CAAC,OAAO;YAAE,OAAO,EAAE,CAAC;QAExB,MAAM,GAAG,GAAG,OAAO,CAAC,IAAI,EAAE,CAAC,KAAK,CAAC,GAAG,CAAC,CAAC;QACtC,IAAI,GAAG,CAAC,MAAM,KAAK,CAAC;YAAE,OAAO,GAAG,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;YAClC,OAAO,GAAG,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,GAAG,GAAG,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACpC,CAAC,EAAE,CAAC,OAAO,CAAC,CAAC,CAAC;IAEd,OAAO,KAAK,IAAI,CAAC,GAAG,CAAC,CAAC,CAAC,CACrB,oBAAC,cAAc,kBAAC,OAAO,EAAE,OAAO,EAAE,IAAI,EAAE,IAAI,IAAM,KAAK;QACrD,oBAAC,WAAW,QAAE,WAAW,EAAE,WAAW,EAAE,CAAe,CACxC,CAClB,CAAC,CAAC,CAAC,CACF,oBAAC,MAAM,kBACL,GAAG,EAAE,GAAG,EACR,IAAI,EAAE,IAAI,EACV,GAAG,EAAC,YAAY,EAChB,OAAO,EAAE,OAAO,EAChB,OAAO,EAAE,GAAG,EAAE,CAAC,QAAQ,CAAC,IAAI,CAAC,IACzB,KAAK,EACT,CACH,CAAC;AACJ,CAAC,CAAC;AAMF,MAAM,KAAK,GAAG;IACZ,KAAK,EAAE,oBAAoB;IAC3B,MAAM,EAAE,sBAAsB;IAC9B,GAAG,EAAE,sBAAsB;IAC3B,IAAI,EAAE,sBAAsB;CAC7B,CAAC;AAEF,MAAM,MAAM,GAAG,MAAM,CAAC,GAAG,CAAC,KAAK,CAAC,CAAC,EAAC,IAAI,EAAkB,EAAE,EAAE,CAAC,CAAC;IAC5D,SAAS,EAAE,GAAG,IAAI,KAAK,OAAO,IAAI,KAAK,CAAC,IAAI,CAAC,eAAyB;CACvE,CAAC,CAAC,CAAiB,EAAE,CAAC;AAEvB,MAAM,cAAc,GAAG,MAAM,CAAC,GAAG,CAAC,KAAK,CAAC,CAAC,EAAC,IAAI,EAAkB,EAAE,EAAE,CAAC,CAAC;IACpE,SAAS,EACP,uEAAuE;QACvE,oCACE,IAAI,KAAK,OAAO,IAAI,KAAK,CAAC,IAAI,CAChC,sBAAsB;CACzB,CAAC,CAAC,CAAiB,EAAE,CAAC;AAEvB,MAAM,WAAW,GAAG,MAAM,CAAC,CAAC,CAAC,KAAK,CAAC;IACjC,SAAS,EAAE,0CAA0C;CACtD,CAAC,CAAA,EAAE,CAAC\"}"}
